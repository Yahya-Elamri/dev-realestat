{
  "name": "realestate-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docker:build": "docker build -t realestate-frontend .",
    "docker:run": "docker run -d -p 3000:80 --name realestate-frontend realestate-frontend",
    "docker:compose": "docker-compose up -d",
    "docker:stop": "docker stop realestate-frontend && docker rm realestate-frontend",
    "docker:logs": "docker logs realestate-frontend",
    "docker:dev": "docker-compose -f ../docker-compose.yml up -d"
  },
  "dependencies": {
    "axios": "^1.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}